{
  "name": "gittip-links",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "show user has gittip account in Github, Twitter and Bitbucket",
  "icons": {
    "16": "images/icon_016.png",
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "components/jquery/jquery.min.js",
        "components/lodash/lodash.js",
        "js/gittip.js"
      ],
      "css": [
      ]
    }
  ],
  "web_accessible_resources": [
    "images/icon_016.png"
  ],
  "permissions": [
    "https://www.gittip.com/"
  ]
}